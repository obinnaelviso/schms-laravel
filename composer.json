{
    "name": "changeweb/unifiedtransform",
    "description": "A school management software.",
    "keywords": ["school", "management"],
    "license": "GPL 3",
    "type": "project",
    "require": {
        "php": ">=8.0.2",
        "doctrine/dbal": "^3.5",
        "jdavidbakr/laravel-cache-garbage-collector": "^2.0",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/cashier": "^14.0",
        "laravel/framework": "^9.0",
        "laravel/passport": "^11.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mavinoo/laravel-batch": "2.3.4",
        "renatomarinho/laravel-page-speed": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^3.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^7",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^9.0",
        "rap2hpoutre/laravel-log-viewer": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/utilities/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk",
                "barryvdh/laravel-debugbar",
                "rap2hpoutre/laravel-log-viewer"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "@php artisan passport:keys"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
